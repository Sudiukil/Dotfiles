#!/bin/zsh

# cheat.sh shortcut
function cheatsheet { curl http://cheat.sh/$1 }

# Grep processes, excluding the grep process with grep (grep!)
function pgrep {
  ps -aux | grep $1 | grep -v grep | trim # see aliases
}

# Cut space-separated columns easily
function col {
  tr -s ' ' | cut -d ' ' -f $1
}
